{
  "name": "calorie-calculator-svc",
  "version": "1.0.0",
  "description": "Hey! I count total calories in a day.",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run remove-dist",
    "build": "tsc",
    "postbuild": "npm run copy-resources",
    "prestart": "npm run remove-dist",
    "start": "tsc-watch --project . --onSuccess \"npm run nodemon\"",
    "prenodemon": "npm run copy-resources",
    "nodemon": "nodemon ./dist/index.js",
    "remove-dist": "rm -rf ./dist",
    "copy-resources": "cp -Rf ./src ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yaminichauhan/calorie-calculator-svc.git"
  },
  "author": "Yamini Chauhan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yaminichauhan/calorie-calculator-svc/issues"
  },
  "homepage": "https://github.com/yaminichauhan/calorie-calculator-svc#readme",
  "dependencies": {
    "bcrypt": "5.0.1",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "mongoose": "6.0.12",
    "tsc-watch": "4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.0",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/node": "16.11.6",
    "mongodb": "4.1.3",
    "nodemon": "2.0.14",
    "typescript": "4.4.4"
  }
}
